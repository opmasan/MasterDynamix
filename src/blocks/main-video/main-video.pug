mixin b-main-video()
	.section-video.section
		video#myVideo(loop='', muted='', data-autoplay='')
			source(src='assets/images/coding.mp4', type='video/mp4')
		.layer
			.flex-column
				h1 Qualified software development services
				#arrowAnim
					.arrowSliding
						.fas.fa-chevron-down
					.arrowSliding.delay1
						.fas.fa-chevron-down
					.arrowSliding.delay2
						.fas.fa-chevron-down
